{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/404\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/register\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/roleBasedRedirect\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/signin\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/signout\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/updateProfileImage\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/cursos\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/cursos\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/middleware\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/roles\\/dashboardAdmin\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/roles\\/dashboardGuest\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/roles\\/dashboardUser\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/routes\\/about\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/routes\\/dashboard\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/routes\\/profile\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/routes\\/profileAdmin\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/routes\\/profileGuest\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/routes\\/profileUser\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/signin\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "/.*",
			"dest": "_render",
			"status": 404
		}
	]
}